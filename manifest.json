{
    "name": "AJ's New Tab",
    "version": "1.1",
    "manifest_version": 2,
    "description": "AJ's Clean New Tab",
    "chrome_url_overrides": {
      "newtab": "index.html"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "permissions": [
      "history",
      "sessions",
      "tabs",
      "file:///*",
      "storage"
    ],
    "icons": {
           "48": "images/icon48.png"
    },"browser_action": {
    "default_icon": "images/icon48.png",
    "16": "images/blue.png"
  }

}
